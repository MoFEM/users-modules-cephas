
# Set KSP solver

-ksp_type fgmres 
-pc_type lu 
-pc_factor_mat_solver_package mumps
-ksp_monitor -ksp_atol 1e-10 -ksp_rtol 1e-10 

# Set SNES solver

-snes_monitor 
-snes_type newtonls 
-snes_linesearch_type l2 
-snes_linesearch_monitor 
-snes_max_it 16 
-snes_atol 1e-8 
-snes_rtol 1e-8 
-snes_converged_reason 
