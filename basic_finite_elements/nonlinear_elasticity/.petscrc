 # Set KSP solver
 
-ksp_type fgmres 
-pc_type lu 
-pc_factor_mat_solver_package mumps 
-ksp_atol 1e-10 
-ksp_rtol 1e-10 

# Set SNES solver

-snes_monitor 
-snes_type newtonls 
-snes_linesearch_type basic 
-snes_max_it 100 
-snes_atol 1e-7 
-snes_rtol 1e-7 

# Set TS solver

-ts_monitor 
-ts_type alpha 



