# Set KSP solver
  
  
-ksp_type fgmres 
-pc_type lu 
-pc_factor_mat_solver_package mumps 
-ksp_monitor 

# Set SNES solver

-snes_type newtonls 
-snes_linesearch_type basic 
-snes_max_it 100 
-snes_atol 1e-8 
-snes_rtol 1e-8 
-snes_monitor 

# Set TS solver

-ts_monitor 
-ts_type beuler 
-ts_exact_final_time stepover 
